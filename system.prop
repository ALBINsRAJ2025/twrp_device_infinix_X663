ro.hardware=mt6768
ro.mediatek.platform=MT6768
ro.treble.enabled=true

# Decryption support
ro.crypto.metadata.enabled=true
ro.crypto.support_metadata_encrypt=true
ro.crypto.uses_fs_ioc_add_encryption_key=true
ro.crypto.volume.filenames_mode=aes-256-cts

# USB config
persist.sys.usb.config=adb

# Disable crashing Trustonic HALs
ro.hardware.gatekeeper=none
ro.hardware.keymaster=none
ro.hardware.keystore=none
ro.hardware.kmsetkey=none
